{
    "name": "ohanspace/sms",
    "type": "library",
    "description": "this is a sms package made for laravel",
    "keywords": [
        "sms"
    ],
    "homepage": "https://github.com/ohanspace/sms",
    "license": "MIT",
    "authors": [
        {
            "name": "Borhan Chowdhury",
            "email": "borhan.chittagong@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Ohanspace\\Sms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ohanspace\\Sms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
